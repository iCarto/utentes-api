<!-- tmpl block info modal -->
<script type="text/template" id="block-info-modal-tmpl">
<div
    id="editInfoModal"
    class="modal fade"
    tabindex="-1"
    role="dialog"
    aria-labelledby="editInfoModalLabel"
>
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button
                    type="button"
                    class="close"
                    data-dismiss="modal"
                    aria-label="Pechar"
                >
                    <span aria-hidden="true">&times;</span>
                </button>
                <h4 id="editInfoModalLabel" class="modal-title">Exploração</h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-xs-offset-1 col-xs-10">
                        <div class="panel panel-info">
                            <div class="panel-heading">
                                <h3 class="panel-title"><strong>Informação</strong></h3>
                            </div>
                            <div class="panel-body">
                                <div class="row">
                                    <div class="form-group col-xs-6">
                                        <label for="exp_id">Número da exploração</label>
                                        <input
                                            type="text"
                                            id="exp_id"
                                            class="form-control widget uilib-enability uilib-enable-role-administrador uilib-enable-role-tecnico"
                                            required
                                        >
                                    </div>
                                    {% if request.registry.settings.get("ara") != 'DPMAIP' %}
                                    <div class="form-group col-xs-6">
                                        <label for="d_soli">{{ d_soli_label }}</label>
                                        <input
                                            type="text"
                                            id="d_soli"
                                            class="form-control widget-date uilib-enability uilib-enable-role-administrador uilib-enable-role-tecnico"
                                            placeholder="dd/mm/yyyy"
                                        >
                                    </div>
                                    {% endif %}
                                </div>

                                <div class="row">
                                    <div class="form-group col-xs-6">
                                        <label for="exp_name">Nome da exploração</label>
                                        <input
                                            type="text"
                                            id="exp_name"
                                            class="form-control widget uilib-enability uilib-enable-role-administrador uilib-enable-role-tecnico"
                                            required
                                        >
                                    </div>
                                    <div class="form-group col-xs-6">
                                        <label for="cadastro_uni">
                                            Número do cadastro unificado
                                        </label>
                                        <input
                                            type="text"
                                            id="cadastro_uni"
                                            class="form-control widget uilib-enability uilib-enable-role-administrador uilib-enable-role-tecnico"
                                        >
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-xs-offset-1 col-xs-10">
                        <div class="panel panel-info">
                            <div class="panel-heading">
                                <h3 class="panel-title"><strong>DUAT</strong></h3>
                            </div>
                            <div class="panel-body">
                                <div class="row">
                                    <div class="form-group col-xs-6">
                                        <label for="d_titulo">Número do título</label>
                                        <input
                                            type="text"
                                            id="d_titulo"
                                            class="form-control widget uilib-enability uilib-enable-role-administrador uilib-enable-role-juridico"
                                        >
                                    </div>
                                    <div class="form-group col-xs-6">
                                        <label for="d_proceso">
                                            Número do processo
                                        </label>
                                        <input
                                            type="text"
                                            id="d_proceso"
                                            class="form-control widget uilib-enability uilib-enable-role-administrador uilib-enable-role-juridico"
                                        >
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="form-group col-xs-6">
                                        <label for="d_folha">Número de folha</label>
                                        <input
                                            type="text"
                                            id="d_folha"
                                            class="form-control widget uilib-enability uilib-enable-role-administrador uilib-enable-role-juridico"
                                        >
                                    </div>
                                    <div class="form-group col-xs-6">
                                        <label for="d_parcela">Número de parcela</label>
                                        <input
                                            type="text"
                                            id="d_parcela"
                                            class="form-control widget uilib-enability uilib-enable-role-administrador uilib-enable-role-juridico"
                                        >
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="form-group col-xs-6">
                                        <label for="d_area">Área <i class="units">(ha)</i></label>
                                        <input
                                            type="text"
                                            id="d_area"
                                            class="form-control widget-number uilib-enability uilib-enable-role-administrador uilib-enable-role-juridico"
                                            pattern="[0-9]{1,8}([,][0-9]{1,4})?"
                                        >
                                    </div>
                                    <div class="form-group col-xs-6">
                                        <label for="d_d_emis">Data de emissão </label>
                                        <input
                                            type="text"
                                            id="d_d_emis"
                                            class="form-control widget-date uilib-enability uilib-enable-role-administrador uilib-enable-role-juridico"
                                            placeholder="dd/mm/yyyy"
                                        >
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="form-group col-xs-6">
                                        <label for="d_l_emis">Local de emissão</label>
                                        <input
                                            type="text"
                                            id="d_l_emis"
                                            class="form-control widget uilib-enability uilib-enable-role-administrador uilib-enable-role-juridico"
                                        >
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                          <div class="form-group col-xs-12">
                            <label for="observacio">Observaçoes sobre linha de água e localizaçao</label>
                            <textarea class="form-control widget uilib-enability uilib-enable-role-administrador uilib-enable-role-tecnico" id="observacio" rows="3"></textarea>
                          </div>
                        </div>
                    </div>
                </div>
            </div>
            {{exploracao_show_macros.modal_footer()}}
        </div>
    </div>
</div>
</script>

                        <label for="c_soli">Consumo solicitado <i class="units">(m<sup>3</sup>/mês)</i></label>
                        <input type="text" class="form-control widget-number" id="c_soli" pattern="[0-9]{1,8}([,][0-9]{1,2})?">
                      </div>
                      <div class="form-group col-xs-6">
                        <label for="c_real">Consumo real <i class="units">(m<sup>3</sup>/mês)</i></label>
                        <input type="text" class="form-control widget-number" id="c_real" pattern="[0-9]{1,8}([,][0-9]{1,2})?">
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <div class="row">
              <div class="form-group col-xs-offset-1 col-xs-10">
                <label for="observacio">Observaçôes</label>
                <textarea class="form-control widget" id="observacio" rows="3"></textarea>
              </div>
            </div>
          </div>
          <div class="modal-footer">
            <div class="row">
              <div class="col-xs-offset-1 col-xs-10">
                <button type="button" class="btn btn-default" data-dismiss="modal">Cancelar</button>
                <button type="button" class="btn btn-primary" id="okButton">Aceitar</button>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </script>

  <!-- template licencia -->
  <script type="text/template" id="licencia-tmpl">
    <div class="row">
      <h4 class="col-xs-offset-1 col-xs-11">Licença <%- tipo_agua + ': ' + (lic_nro || '-') %>
        <button type="button" class="btn btn-default btn-xs hidden uilib-enability uilib-show-role-administrador uilib-show-role-tecnico" id="addLicense">Adicionar</button>
        <button type="button" class="btn btn-default btn-xs uilib-enability uilib-show-role-administrador uilib-show-role-tecnico" id="editLicense">Editar</button>
        <button type="button" class="btn btn-default btn-xs uilib-enability uilib-show-role-administrador uilib-show-role-tecnico" id="addFonte">Nova fonte</button>
        <button type="button" class="btn btn-default btn-xs uilib-enability uilib-show-role-administrador uilib-show-role-tecnico" id="removeLicense">Eliminar</button>
      </h4>
    </div>

    <div class="row">
      <span class="col-xs-offset-1 col-xs-11">
        <span class="title">Estado:</span> <%- estado || '-' %> <i id="info-estado-licencia" class="fa fa-question-circle" style="cursor:pointer"></i>
      </span>
    </div>
    <div class="row">
      <span class="col-xs-offset-1 col-xs-11">
        <span class="title">Nro de cadastro:</span> <%- cadastro || '-' %>
        <span class="title">Nro de licença histórico:</span> <%- n_licen_a || '-' %>
        <span class="title">Tipo de Licença:</span> <%- tipo_lic || '-' %>
      </span>
    </div>
    <div class="row">
      <span class="col-xs-offset-1 col-xs-11">
        <span class="title">Data emissão:</span> <%- formatter().formatDate(d_emissao) || '-' %>
        <span class="title">Data validade:</span> <%- formatter().formatDate(d_validade) || '-' %>
      </span>
    </div>
    <div class="row">
      <span class="col-xs-offset-1 col-xs-11">
        <span class="title">Valor pago mês <i class="units">(MZN/més)</i></span> <%- formatter().formatNumber(pago_mes) || '-' %>
        <span class="title">IVA <i class="units">(%)</i></span> <%- formatter().formatNumber(iva) || '-' %>
        <span class="title">Valor com IVA <i class="units">(MZN/més)</i></span> <%- formatter().formatNumber(pago_iva) || '-' %>
      </span>
    </div>
    <div class="row">
      <span class="col-xs-offset-1 col-xs-11">
        <span class="title">Tipo de consumo</span> <%- consumo_tipo || '-' %>
        <span class="title">Taxa fixa <i class="units">(MZN/més)</i></span> <%- formatter().formatNumber(taxa_fixa) || '-' %>
        <span class="title">Taxa uso <i class="units">(MZN/m<sup>3</sup>)</i></span> <%- formatter().formatNumber(taxa_uso) || '-' %>
      </span>
    </div>


    <div class="row">
      <span class="col-xs-offset-1 col-xs-2 text-right"><%- formatter().formatNumber(consumo_fact) || '-' %> </span>
      <span class="col-xs-9">
        <span class="title"><i class="units">m<sup>3</sup>/mês</i> consumo facturado</span> <span class="title">(Tipo de facturação: <span class="js_fact_tipo"></span>)</span>
      </span>
    </div>
    <div class="row">
      <span class="col-xs-offset-1 col-xs-2 text-right"><%- formatter().formatNumber(c_licencia) || '-' %> </span>
      <span class="col-xs-9">
        <span class="title"><i class="units">m<sup>3</sup>/mês</i> consumo licenciado</span>
      </span>
    </div>

    <div class="row">
      <span class="col-xs-offset-1 col-xs-2 text-right"><%- formatter().formatNumber(c_real_tot) || '-' %> </span>
      <span class="col-xs-9">
        <span class="title"><i class="units">m<sup>3</sup>/mês</i> consumo real</span> <span class="title">(<%- formatter().formatNumber(c_real_int) || '-' %> intermedio / <%- formatter().formatNumber(c_real_fon) || '-' %> fontes)</span>
      </span>
    </div>
    <div class="row">
      <span class="col-xs-offset-1 col-xs-2 text-right"><%- formatter().formatNumber(c_soli_tot) || '-' %> </span>
      <span class="col-xs-9">
        <span class="title"><i class="units">m<sup>3</sup>/mês</i> consumo solicitado</span> <span class="title">(<%- formatter().formatNumber(c_soli_int) || '-' %> intermedio / <%- formatter().formatNumber(c_soli_fon) || '-' %> fontes)</span>
      </span>
    </div>

  </script>

